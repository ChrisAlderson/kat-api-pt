{
  "name": "kat-api-pt",
  "version": "1.0.7",
  "description": "A KickassTorrents api wrapper heavily based on the kat-api module of paulhobbel.",
  "dependencies": {
    "bytes": "^2.3.0",
    "cheerio": "^0.20.0",
    "request": "^2.69.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-plugin-transform-strict-mode": "^6.8.0",
    "babel-preset-es2015": "^6.6.0"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel index.es6 -o index.js",
    "prepublish": "npm run build"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "keywords": [
    "kat",
    "kickass",
    "torrent",
    "api",
    "popcorn"
  ],
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-strict-mode",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ]
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisAlderson/kat-api-pt.git"
  }
}
